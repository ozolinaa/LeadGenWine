@model LeadGen.Code.Lead.LeadItem
<div>
    @if (Model.businessDetails.businessContactReceivedDateTime != null)
    {
        @Html.Partial("LeadActionParts/_Contacts", Model)
    }
    else if (Model.businessDetails.isPermittedForBusiness == false)
    {
        @Html.Partial("LeadActionParts/_WaitingApproval", Model)
    }

    @if (Model.businessDetails.businessCompletedDateTime != null)
    {
        @Html.Partial("LeadActionParts/_Completed", Model)
    }
</div>